project_name: bluetooth_dosimeter_consumer
label: bluetooth_dosimeter_consumer
description: |
  Implements a Dosimeter Consumer device which get dosimeter data from Dosimeter Sender and process collected data.
category: Bluetooth Examples
filter:
- name: Device Type
  value: [SoC]
- name: Project Difficulty
  value: [Beginner]
- name: Wireless Technology
  value: [Bluetooth]
package: Bluetooth
quality: development
readme:
- path: ../../../documentation/example/bluetooth_dosimeter/README.md
source:
- path: main.c
- path: app.c
- path: consumer_app.c
- path: consumer_nvm.c
- path: consumer_oled_app.c
- path: custom_font.c
include:
- path: ''
  file_list:
  - path: app.h
  - path: consumer_app.h
  - path: consumer_nvm.h
  - path: consumer_oled_app.h
  - path: custom_font.h
component:
- instance: [btn0]
  id: simple_button
- id: bluetooth_feature_gatt_server
- id: in_place_ota_dfu
- id: bluetooth_feature_sm
- id: simple_timer
- id: mpu
- id: bluetooth_feature_legacy_advertiser
- id: gatt_configuration
- id: bluetooth_stack
- id: rail_util_pti
- id: bluetooth_feature_gatt
- id: brd4314a
- id: BGM220PC22HNA
- id: bluetooth_feature_legacy_scanner
- id: app_log}
- id: bluetooth_feature_connection
- instance: [vcom]
  id: iostream_usart
- id: bluetooth_feature_system
- id: printf
- id: component_catalog
- {from: third_party_hw_drivers, id: services_oled_glib}
- {from: third_party_hw_drivers, id: mikroe_buzz2}
- {from: third_party_hw_drivers, id: sparkfun_micro_oled_ssd1306}
- id: bootloader_interface
- id: app_assert
config_file:
  - override:
      component: gatt_configuration
      file_id: gatt_configuration_file_id
    path: gatt_configuration.btconf
    directory: btconf
other_file:
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_btn.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_config.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_event.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_init.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_logic.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_normal.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_notify.png
  - path: ../../../documentation/example/bluetooth_dosimeter/consumer_overview.png
  - path: ../../../documentation/example/bluetooth_dosimeter/create_example.png
  - path: ../../../documentation/example/bluetooth_dosimeter/display.png
  - path: ../../../documentation/example/bluetooth_dosimeter/hardware_connection.png
  - path: ../../../documentation/example/bluetooth_dosimeter/overview.png
  - path: ../../../documentation/example/bluetooth_dosimeter/sender_callback.png
  - path: ../../../documentation/example/bluetooth_dosimeter/sender_init.png
  - path: ../../../documentation/example/bluetooth_dosimeter/sender_overview.png
  - path: ../../../documentation/example/bluetooth_dosimeter/log_sender.png
  - path: ../../../documentation/example/bluetooth_dosimeter/log_consumer.png
  - path: ../../../documentation/example/bluetooth_dosimeter/efr_connect.png
  - path: ../../../documentation/example/bluetooth_dosimeter/configuration.png
  - path: ../../../documentation/example/bluetooth_dosimeter/disconnected.GIF
configuration:
- {name: SL_STACK_SIZE, value: '2752'}
- {name: SL_HEAP_SIZE, value: '9200'}
- condition: [psa_crypto]
  name: SL_PSA_KEY_USER_SLOT_COUNT
  value: '0'
ui_hints:
  highlight:
  - path: README.md
    focus: true
  - path: config/btconf/gatt_configuration.btconf
sdk_extension:
- id: third_party_hw_drivers
  version: 1.0.0

